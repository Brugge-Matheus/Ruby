{
    "editor.fontFamily": "JetBrains Mono",
    "chat.editor.fontSize": 14,
    "editor.fontLigatures": true,
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "php.validate.executablePath": "",
    "editor.guides.bracketPairs": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.statusBar.visible": false,
    "symbols.hidesExplorerArrows": false,
    "editor.lineHeight": 1.6,
    "editor.renderLineHighlight": "gutter",
    "workbench.editor.labelFormat": "short",
    "explorer.compactFolders": false,
    "workbench.activityBar.location": "bottom",
    "editor.rulers": [
        80,
        120
    ],
    "editor.scrollbar.horizontal": "hidden",
    "apc.electron": {
        "titleBarStyle": "hiddenInset",
    },
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "apc.listRow": {
        "height": 24
    },
    "apc.stylesheet": {
        ".pane-body": "padding 8px"
    },
    "explorer.fileNesting.enabled": true,
    "terminal.integrated.fontSize": 14,
    "editor.minimap.enabled": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.saveAndFormat",
            "sequence": [
                "workbench.action.files.save",
                "editor.action.formatDocument"
            ]
        }
    ],
    "security.allowedUNCHosts": [
        "web.agencia.red@SSL@2078",
        "wsl.localhost"
    ],
    "database-client.autoSync": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "lldb.suppressUpdateNotifications": true,
    "editor.wordWrap": "on",
    "liveServer.settings.donotShowInfoMsg": true,
    "phpParameterHint.hintOnlyLiterals": true,
    "workbench.colorTheme": "Min Dark",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "update.showReleaseNotes": false,
    "editor.inlineSuggest.suppressSuggestions": true,
    "cody.autocomplete.languages": {
        "*": false
    },
    "cody.commandHints.enabled": false,
    "cody.codeActions.enabled": false,
    "cody.autocomplete.completeSuggestWidgetSelection": false,
    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "editor.fontSize": 18
}